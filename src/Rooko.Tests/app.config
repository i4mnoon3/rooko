<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<!-- Example connection to a SQL Server Database on localhost. -->
		<!-- <add name="ExampleConnectionString"
			connectionString="Data Source=.;Initial Catalog=DBName;Integrated Security=True"
			providerName="System.Data.SqlClient" /> -->
		<add name="dev-mysql" connectionString="server=localhost;user id=root;database=test" providerName="MySql.Data.MySqlClient"/>
		<add name="dev-sqlite" connectionString="data source=db.sqlite" providerName="System.Data.SQLite"/>
	</connectionStrings>
	<appSettings>
		<!-- access these values via the property:
			System.Configuration.ConfigurationManager.AppSettings[key]
		-->
		<add key="database" value="dev-sqlite" />
		<add key="Setting2" value="Easy" />
	</appSettings>
</configuration>
